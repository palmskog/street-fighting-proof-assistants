n = read_int();

a = alloc(n, 0);
i = 0;
while (i < n) {
  a[i] = n - i;
  i = i + 1;
}

pivot = n / 2;

# Partition `a` into three chunks: <pivot, =pivot, >pivot

l = 0;  # end of first chunk; always <= i
h = len(a);  # start of third chunk; always >= i

i = 0;
while (i < h) {
  if (a[i] < pivot) {
    tmp = a[l]; a[l] = a[i]; a[i] = tmp;
    l = l + 1;
    i = i + 1;    
  } else { 
    if (a[i] == pivot) {
      i = i + 1;
    } else { # a[i] > pivot
      tmp = a[h - 1]; a[h - 1] = a[i]; a[i] = tmp;
      h = h - 1;
    }
  }
}

result = a;

return result;